#include <stdio.h>
#include <stdlib.h>

typedef struct node{
    int data;
    struct node* next;
} Node;

typedef struct queue{
    Node* front;
    Node* rear;
    int size;
} Queue;


Queue* initQueue(){
    Queue* newQueue = (Queue*) malloc(sizeof(Queue));
    newQueue->front = NULL;
    newQueue->rear = NULL;
    newQueue->size = 0;
    
    return newQueue;
}



// add element
void enqueue(Queue* queue, int data){
    
    Node* newNode = (Node*) malloc(sizeof(Node));
    if (newNode == NULL)
        return;
        
    newNode->data = data;
    newNode->next = NULL;
    
    if (queue->front == NULL){
        queue->front = newNode;
        queue->rear = newNode;
        queue->size++;
        return;
    }
    
    queue->rear->next = newNode;
    queue->rear = newNode;
    queue->size++;
}
// remove first (FIFO)
void dequeue(Queue* queue){
    if (queue->front == NULL)
        return;

    Node* aux = queue->front;
    queue->front = queue->front->next;
    if (queue->front == NULL)
        queue->rear = NULL;
    queue->size--;
    free(aux);
}

void printQueue(Queue* queue){
    Node* nodeToPrint = queue->front;
    
    while(nodeToPrint != NULL){
        printf("Data: %d\n", nodeToPrint->data);
        nodeToPrint = nodeToPrint->next;
    }
}

void freeQueue(Queue* queue){
    Node* nodeToDelete;
    Node* aux = queue->front;
    
    while(aux != NULL){
        nodeToDelete = aux;
        aux = aux->next;
        free(nodeToDelete);
    }
    free(queue);
}

int main()
{
    Queue* queue = initQueue();
    
    enqueue(queue, 10);
    enqueue(queue, 20);
    enqueue(queue, 30);
    enqueue(queue, 40);
    
    dequeue(queue);
    dequeue(queue);
    
    printQueue(queue);

    freeQueue(queue);
	return 0;
}
